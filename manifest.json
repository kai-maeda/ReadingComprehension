{
    "name" : "VocabHub",
    "version":"1.0.0",
    "manifest_version": 3,
    "permissions":[
    "activeTab",
    "tabs",
    "storage"
],

"content_scripts":[
    {
        "matches":["<all_urls>"],
        "js":["main.js"],
        "run_at":"document_end"
    }
],
"action": {
    "default_title": "VocabHub",
    "default_popup": "main.html"
},
"background":{
    "service-worker":"firebase.js"
}
}

